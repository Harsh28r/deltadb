{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3ExpressListBuckets",
      "Effect": "Allow",
      "Action": [
        "s3:ListAllMyBuckets"
      ],
      "Resource": "*"
    },
    {
      "Sid": "S3ExpressSessionPermissions",
      "Effect": "Allow",
      "Action": [
        "s3express:CreateSession"
      ],
      "Resource": "arn:aws:s3express:eu-north-1:942237908667:bucket/deltayards-crm-file--eun1-az1--x-s3"
    },
    {
      "Sid": "S3ExpressBucketOperations",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": "arn:aws:s3express:eu-north-1:942237908667:bucket/deltayards-crm-file--eun1-az1--x-s3"
    },
    {
      "Sid": "S3ExpressObjectOperations",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:GetObjectAttributes"
      ],
      "Resource": "arn:aws:s3express:eu-north-1:942237908667:bucket/deltayards-crm-file--eun1-az1--x-s3/*"
    }
  ]
}
